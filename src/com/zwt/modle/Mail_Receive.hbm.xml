<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zwt.modle.Mail_Receive" table="t_mail_receive">
		<id name="id" column="id"><!-- 单一主键 -->
			<generator class="native" />
		</id>
		
		<property name="mailTitle" column="mailTitle"/>
		<property name="mailContent" column="mailContent"/>
		<property name="nature" column="nature"/>
		<property name="isStar" column="isStar"/>
		<property name="status" column="status"/>
		<property name="local" column="local"/>
		<property name="sendPeople" column="sendPeople"/>
		<property name="receivePeople" column="receivePeople"/>
		<property name="dateTime" column="dateTime"/>
		
	</class>
</hibernate-mapping>